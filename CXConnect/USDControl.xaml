<USD:DynamicsBaseHostedControl x:Class="CXConnect.USDControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:USD="clr-namespace:Microsoft.Crm.UnifiedServiceDesk.Dynamics;assembly=Microsoft.Crm.UnifiedServiceDesk.Dynamics"
             xmlns:gif="https://github.com/XamlAnimatedGif/XamlAnimatedGif"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"  
             xmlns:materialDesignConverters="clr-namespace:MaterialDesignThemes.Wpf.Converters;assembly=MaterialDesignThemes.Wpf"
             xmlns:local="clr-namespace:CXConnect"
             mc:Ignorable="d"
             Unloaded="DynamicsBaseHostedControl_Unloaded" MaxHeight="600" MaxWidth="330">
    
    <USD:DynamicsBaseHostedControl.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Light.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Primary/MaterialDesignColor.Grey.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Accent/MaterialDesignColor.Lime.xaml" />

                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Button.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.CheckBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.ListBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.PopupBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.RadioButton.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.TextBlock.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.ToggleButton.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.ComboBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Slider.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.TextBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.PasswordBox.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Expander.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Menu.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.DataGrid.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.ToggleButton.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Chip.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.ToolBar.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
            <local:VariableLocator x:Key="VariableLocator"/>
            <local:DialogExtensionLocator x:Key="DialogExtensionLocator"/>
            <local:RecentLocator x:Key="RecentLocator"/>
            <materialDesignConverters:BooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter" TrueValue="Collapsed" FalseValue="Visible" />
        </ResourceDictionary>
    </USD:DynamicsBaseHostedControl.Resources>
    <Grid Margin="0,0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <DockPanel Grid.Row="0" Background="{StaticResource PrimaryHueDarkBrush}" Margin="0,0">
            <!-- MouseDown="DockPanel_MouseDown"-->
            <Grid DockPanel.Dock="Left" HorizontalAlignment="Left">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                    <materialDesign:PackIcon Kind="HeadphonesSettings" Foreground="{Binding AgentInformation.SelectedVoiceStatus,Converter={StaticResource VariableLocator},ConverterParameter=IconColor}"  Margin="7,0"/>
                    <TextBlock Text="{Binding AgentInformation.Name}" Foreground="{StaticResource PrimaryHueLightBrush}"  Margin="3,0"/>
                    <TextBlock Text="(" Foreground="{StaticResource PrimaryHueLightBrush}"  Margin="0,0"/>
                    <TextBlock Text="{Binding AgentInformation.Extension}" Foreground="{StaticResource PrimaryHueLightBrush}"  Margin="1,0"/>
                    <TextBlock Text=")" Foreground="{StaticResource PrimaryHueLightBrush}"  Margin="0,0"/>
                </StackPanel>
            </Grid>
            <!--<Grid DockPanel.Dock="Right" HorizontalAlignment="Right">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center">
                    <Button x:Name="MinimizeButton" Style="{StaticResource MaterialDesignRaisedDarkButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Background="Transparent" BorderBrush="Transparent" KeyboardNavigation.IsTabStop="False" Click="MinimizeButton_Click" Margin="7,0" Padding="5,5">
                        <materialDesign:PackIcon Kind="WindowMinimize" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                    </Button>
                    <Button x:Name="MaximizeButton" Style="{StaticResource MaterialDesignRaisedDarkButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Background="Transparent" BorderBrush="Transparent" KeyboardNavigation.IsTabStop="False" Click="MaximizeButton_Click" Margin="7,0" Padding="5,5">
                        <materialDesign:PackIcon Kind="WindowMaximize" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                    </Button>
                    <Button x:Name="CloseButton" Style="{StaticResource MaterialDesignRaisedDarkButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Background="Transparent" BorderBrush="Transparent" KeyboardNavigation.IsTabStop="False" Click="Window_Closing" Margin="7,0" Padding="5,5">
                        <materialDesign:PackIcon Kind="WindowClose" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                    </Button>

                </StackPanel>
            </Grid>-->
        </DockPanel>
        <Grid Name="MainPage" Grid.Row="1" Visibility="Visible">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <Grid Name="MainScreen" Grid.Row="0">
                <Grid Name="DialPadScreen" Margin="45,20,45,10" Visibility="Hidden">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <TextBox Name="NumberToDial" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Margin="0,15,0,20" HorizontalContentAlignment="Center" Text="{Binding Path=AgentInformation.ReservedObject.DialNumber}" FontSize="24" BorderBrush="Transparent" BorderThickness="0" Background="Transparent">
                        <TextBox.Resources>
                            <Style TargetType="TextBox">
                                <Style.Triggers>
                                    <Trigger Property="IsFocused" Value="true">
                                        <Setter Property = "BorderBrush" Value="Transparent"/>
                                        <Setter Property = "BorderThickness" Value="0"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </TextBox.Resources>
                    </TextBox>
                    <Button Name="one" Width="60" Height="60" Grid.Row="1" Grid.Column="0" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="1" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text=" " FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="two" Width="60" Height="60" Grid.Row="1" Grid.Column="1" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28"  Text="2" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Padding="0,0" Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="ABC" FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="three" Width="60" Height="60" Grid.Row="1" Grid.Column="2" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="3" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="DEF" FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="four" Width="60" Height="60" Grid.Row="2" Grid.Column="0" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="4" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="GHI" FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="five" Width="60" Height="60" Grid.Row="2" Grid.Column="1" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="5" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="JKL" FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="six" Width="60" Height="60" Grid.Row="2" Grid.Column="2" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="6" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="MNO" FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="seven" Width="60" Height="60" Grid.Row="3" Grid.Column="0" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="7" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="PQRS" FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="eight" Width="60" Height="60" Grid.Row="3" Grid.Column="1" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="8" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="TUV" FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="nine" Width="60" Height="60" Grid.Row="3" Grid.Column="2" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="9" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="WXYZ" FontSize="9"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="star" Width="60" Height="60" Grid.Row="4" Grid.Column="0" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="34" Text="*" HorizontalAlignment="Center"/>
                    </Button>
                    <Button Name="zero" Width="60" Height="60" Grid.Row="4" Grid.Column="1" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <StackPanel HorizontalAlignment="Center">
                            <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="28" Text="0" HorizontalAlignment="Center"/>
                            <WrapPanel Margin="0,0" HorizontalAlignment="Center">
                                <TextBlock Margin="0,0" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="+"/>
                            </WrapPanel>
                        </StackPanel>
                    </Button>
                    <Button Name="hash" Width="60" Height="60" Grid.Row="4" Grid.Column="2" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionLightButton}" Margin="7,7" Click="MakeDialPadButtonClick">
                        <TextBlock Padding="0,0" Margin="0,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignTitleTextBlock}" FontSize="34" Text="#" HorizontalAlignment="Center"/>
                    </Button>
                    <Button Name="call" Width="60" Height="60" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionAccentButton}" Margin="7,15" Click="MakeDialPadButtonClick" Background="#49d862" BorderBrush="#49d862">
                        <materialDesign:PackIcon Kind="Phone" Foreground="White" Width="36" Height="36"/>
                    </Button>
                </Grid>
                <Grid Name="PhoneScreen" Background="{StaticResource MaterialDesignDarkBackground}" Visibility="Hidden">
                    <!-- Background="{DynamicResource PrimaryHueDarkBrush}" -->
                    <materialDesign:Transitioner SelectedIndex="{Binding Path=AgentInformation.SelectedDialog.ReservedObject,Converter={StaticResource DialogExtensionLocator},ConverterParameter=CallWaiting}" Visibility="{Binding AgentInformation.Dialogs,Converter={StaticResource DialogExtensionLocator},ConverterParameter=ShowCallsPanel,UpdateSourceTrigger=PropertyChanged}">
                        <materialDesign:TransitionerSlide OpeningEffect="{materialDesign:TransitionEffect Kind=ExpandIn}">
                            <materialDesign:Transitioner Name="CallActionsForm" SelectedIndex="0">
                                <materialDesign:TransitionerSlide>
                                    <Grid Name="CallActionsMainForm">
                                        <Grid Name="CallActionDetails" Margin="50,10,50,10" Visibility="{Binding Path=AgentInformation.SelectedDialog.Release,Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="1*"/>
                                            </Grid.ColumnDefinitions>
                                            <ListBox Name="ListCalls" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3"  ItemsSource="{Binding Path=AgentInformation.Dialogs}" ScrollViewer.HorizontalScrollBarVisibility="Disabled" Padding="0,0" SelectedValue="{Binding Path=AgentInformation.SelectedDialog,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" SelectionChanged="Calls_SelectionChanged" ScrollViewer.VerticalScrollBarVisibility="Auto" MaxHeight="100" Height="100" Margin="-30,5,-30,0" BorderBrush="White">
                                                <ListBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <Grid Name="CallHeader" HorizontalAlignment="Stretch">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="*"/>
                                                                <ColumnDefinition Width="auto"/>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Margin="2,2" Grid.Column="1" Style="{StaticResource MaterialDesignSubheadingTextBlock}" Text="{Binding Path=DialogStateTimer.TimerLabel,UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Right"/>
                                                            <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Margin="2,2" Grid.Column="0" Style="{StaticResource MaterialDesignTitleTextBlock}" Text="{Binding Path=Header,UpdateSourceTrigger=PropertyChanged}"/>
                                                            <Line Stroke="Transparent" Stretch="Fill" X2="1"/>
                                                        </Grid>
                                                    </DataTemplate>
                                                </ListBox.ItemTemplate>
                                            </ListBox>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="1" Grid.Column="0" Margin="5,5">
                                                <Button Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="Hold" ToolTip="Hold Call" IsEnabled="{Binding AgentInformation.SelectedDialog.Hold}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="Pause" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="Hold"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="1" Grid.Column="1" Margin="5,5" >
                                                <Button  Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="Keypad" ToolTip="Send DTMF" IsEnabled="{Binding AgentInformation.SelectedDialog.SendDTMF}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="Dialpad" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="Keypad"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="1" Grid.Column="2" Margin="5,5" >
                                                <Button  Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="Resume" ToolTip="Return Call" IsEnabled="{Binding AgentInformation.SelectedDialog.Resume}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="PlayPause" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="Return"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="2" Grid.Column="0" Margin="5,5">
                                                <Button Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="Consult" ToolTip="Consult Call" IsEnabled="{Binding AgentInformation.SelectedDialog.Consult}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="Plus" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="Add call"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="2" Grid.Column="1" Margin="5,5">
                                                <Button Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="Conference" ToolTip="Merge Call" IsEnabled="{Binding AgentInformation.SelectedDialog.Conference}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="CallMerge" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="Merge"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="2" Grid.Column="2" Margin="5,5">
                                                <Button Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="CTransfer" ToolTip="Consult Forward Call" IsEnabled="{Binding AgentInformation.SelectedDialog.CTransfer}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="FastForward" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="Forward"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="3" Grid.Column="0" Margin="5,5">
                                                <Button Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="Transfer" ToolTip="Direct Forward Call" IsEnabled="{Binding AgentInformation.SelectedDialog.Transfer}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="Forward" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="Transfer"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="3" Grid.Column="1" Margin="5,5">
                                                <Button Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="SurveyTransfer" ToolTip="Survey Transfer" IsEnabled="{Binding AgentInformation.SelectedDialog.Transfer}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="EmoticonHappy" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="Survey"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="3" Grid.Column="2" Margin="5,5">
                                                <Button Background="Transparent" Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="IVRTransfer" ToolTip="IVR Transfer" IsEnabled="{Binding Path=AgentInformation.SelectedDialog,Converter={StaticResource DialogExtensionLocator},ConverterParameter=SendToIVRButton}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="FileTree" Width="24" Height="24" HorizontalAlignment="Center" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                                <WrapPanel HorizontalAlignment="Center">
                                                    <TextBlock Foreground="{DynamicResource PrimaryHueLightBrush}" Style="{StaticResource MaterialDesignCaptionTextBlock}" Text="IVR"/>
                                                </WrapPanel>
                                            </StackPanel>
                                            <StackPanel HorizontalAlignment="Center" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" Margin="5,5,5,0">
                                                <Button  Style="{StaticResource MaterialDesignFloatingActionLightButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" Background="Red" BorderBrush="Red" Foreground="White" Tag="{Binding Path=AgentInformation.SelectedDialog.ID}" Name="Release" ToolTip="Release Call" IsEnabled="{Binding AgentInformation.SelectedDialog.Release}" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="PhoneHangup" Width="24" Height="24" HorizontalAlignment="Center"/>
                                                </Button>
                                            </StackPanel>
                                            <StackPanel Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3">
                                                <Button Background="Transparent" Tag="{Binding AgentInformation.SelectedDialog.ID}" Name="CallData" materialDesign:ShadowAssist.ShadowDepth="Depth0" Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}" HorizontalAlignment="Right" Click="CallButton_Click">
                                                    <materialDesign:PackIcon Kind="Attachment" Foreground="{StaticResource PrimaryHueLightBrush}"/>
                                                </Button>
                                            </StackPanel>
                                        </Grid>
                                        <Grid Name="CallActionAnswer" Margin="50,10,50,30" Visibility="{Binding Path=AgentInformation.SelectedDialog.Answer,Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="auto"/>
                                                <RowDefinition Height="auto"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="auto"/>
                                            </Grid.RowDefinitions>
